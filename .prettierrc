{
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "printWidth": 120,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "bracketSameLine": true,
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
        {
            "files": "*.ejs",
            "options": {
                "parser": "html"
            }
        }
    ]
}